{
  "name": "missftp",
  "version": "1.0.0",
  "description": "Rupeek ledger for financial activities and payments",
  "author": "Rupeek",
  "main": "src/index.js",
  "private": true,
  "license": "ISC",
  "engines": {
    "node": ">=10.16.0"
  },
  "scripts": {
    "start": "cross-env NODE_ENV=production pm2 start ./src/index.js",
    "dev": "nodemon ./src/index.js",
    "dev:worker": "nodemon ./src/worker.js",
    "dev:stream": "nodemon ./src/stream.js",
    "dev:consumer": "nodemon consumer.js",
    "dev:migrate": "sequelize db:migrate",
    "dev:seed": "sequelize db:seed:all",
    "lint": "eslint '**/*.js' --ignore-path .eslintignore",
    "lint:fix": "npm run lint -- --fix",
    "postpublish": "git push --tags",
    "docs": "apidoc -i src -o docs",
    "postdocs": "open-cli docs/index.html",
    "deploy:dev": "shipit dev deploy",
    "deploy:qa": "shipit qa deploy",
    "deploy:uat": "shipit uat deploy",
    "deploy:prod": "shipit production deploy"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint",
      "pre-push": "npm run lint"
    }
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:Rupeekapp/sangraha.git"
  },
  "keywords": [
    "express",
    "node",
    "node.js",
    "es6",
    "es7",
    "es8",
    "es2017",
    "mocha",
    "eslint",
    "REST",
    "API",
    "sangraha"
  ],
  "dependencies": {
    "@sentry/node": "^5.5.0",
    "agenda": "0.9.1",
    "aws-sdk": "^2.503.0",
    "bluebird": "^3.5.5",
    "body-parser": "^1.19.0",
    "celebrate": "^10.0.1",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "cross-env": "^5.2.0",
    "csv-parser": "^2.3.1",
    "deepstream.io-client-js": "^2.3.3",
    "dotenv-safe": "^6.1.0",
    "elastic-apm-node": "^2.12.1",
    "emailjs-mime-parser": "^2.0.7",
    "express": "^4.17.1",
    "express-winston": "^3.2.1",
    "file-system": "^2.2.2",
    "helmet": "^3.18.0",
    "http-status": "^1.3.2",
    "json-2-csv": "^3.5.6",
    "json-diff": "^0.5.4",
    "jsonwebtoken": "^8.5.1",
    "jszip": "^3.2.2",
    "lodash": "^4.17.11",
    "mailparser": "^2.7.7",
    "method-override": "^3.0.0",
    "moment": "^2.24.0",
    "mongoose": "^5.6.7",
    "mongoose-paginate-v2": "^1.3.0",
    "multer": "^1.4.2",
    "node-sftp-s3": "0.0.10",
    "node-telegram-bot-api": "^0.30.0",
    "node-xlsx": "^0.15.0",
    "pg": "^7.11.0",
    "pg-hstore": "^2.3.3",
    "redis": "^2.8.0",
    "request": "^2.88.0",
    "request-promise": "^4.2.4",
    "sequelize": "^5.10.1",
    "sqs-consumer": "^5.4.0",
    "ssh2-sftp-client": "^5.0.2",
    "string-similarity": "^3.0.0",
    "unzipper": "^0.10.8",
    "uuid": "^3.3.2",
    "winston": "^3.2.1",
    "winston-daily-rotate-file": "^3.9.0",
